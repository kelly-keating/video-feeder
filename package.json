{
  "name": "rss-feeder",
  "version": "0.0.0",
  "description": "Consuming RSS feeds and storing for later perusal",
  "author": "Kelly Keating",
  "scripts": {
    "start": "node server",
    "build": "npm i && webpack",
    "dev": "webpack --watch & nodemon server",
    "test": "jest --watchAll",
    "knex": "knex --knexfile server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx ."
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.6",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.18.6",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^8.10.0",
    "eslint-config-eda": "^1.2.0",
    "firebase": "^9.10.0",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.3.0",
    "moment": "^2.28.0",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.2.2",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "sass": "^1.50.1",
    "sass-loader": "12.6.0",
    "style-loader": "^3.3.0",
    "superagent": "7.1.1",
    "superagent-xml2jsparser": "^0.1.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  }
}
